root = true

[{}]
empty = true

# https://www.gnu.org/software/bash/manual/html_node/Brace-Expansion.html
[/usr/{ucb/{ex,edit},lib/{ex?.?*,how_ex}}]
charset = latin1

[*.{,txt,md,tex,\,}]
trim_trailing_whitespace = false

[*{-1..3}/***/*.py]
indent_style = space

[*.{js,ts}]
        This is a valid key = 4_450
end_of_line = utf-8

# Also valid keys
[typing/*.py?]
charset = o
v = one

# vim: ft=editorconfig
