{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "editorconfig",
  "rules": {
    "editorconfig": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "preamble"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[#;].*"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pair"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "preamble": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_line"
          }
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "glob"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "glob": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "/"
            },
            {
              "type": "SYMBOL",
              "name": "wildcard"
            },
            {
              "type": "SYMBOL",
              "name": "integer_range"
            },
            {
              "type": "SYMBOL",
              "name": "brace_expansion"
            },
            {
              "type": "SYMBOL",
              "name": "character_choice"
            },
            {
              "type": "SYMBOL",
              "name": "character_escape"
            },
            {
              "type": "PATTERN",
              "value": "[^?*{}\\[\\]/\\r\\n]"
            }
          ]
        }
      }
    },
    "wildcard": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "STRING",
            "value": "**"
          },
          {
            "type": "STRING",
            "value": "?"
          }
        ]
      }
    },
    "character_escape": {
      "type": "PATTERN",
      "value": "\\\\[?*{},\\[\\]\\\\]"
    },
    "brace_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "glob"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "integer_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "start",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_integer_range_start"
            },
            "named": true,
            "value": "integer"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ".."
          }
        },
        {
          "type": "FIELD",
          "name": "end",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "-?\\d+"
              }
            },
            "named": true,
            "value": "integer"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "character": {
      "type": "PATTERN",
      "value": "[^\\]\\r\\n]"
    },
    "character_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "start",
          "content": {
            "type": "SYMBOL",
            "name": "character"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "-"
          }
        },
        {
          "type": "FIELD",
          "name": "end",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[^\\]\\r\\n]"
              }
            },
            "named": true,
            "value": "character"
          }
        }
      ]
    },
    "character_choice": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "STRING",
                "value": "!"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "character"
              },
              {
                "type": "SYMBOL",
                "name": "character_range"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "\\\\[-\\]\\\\]"
                },
                "named": true,
                "value": "character_escape"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[ \\t]"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "string"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "property": {
      "type": "PATTERN",
      "value": "[^\\s=#;\\[]([^\\n\\r=]*[^\\s=])?"
    },
    "string": {
      "type": "PATTERN",
      "value": "\\S([^\\n\\r]*\\S)?"
    },
    "_eol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        },
        {
          "type": "SYMBOL",
          "name": "_end_of_file"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_end_of_file"
    },
    {
      "type": "SYMBOL",
      "name": "_integer_range_start"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}