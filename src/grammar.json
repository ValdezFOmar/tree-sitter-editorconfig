{
  "name": "editorconfig",
  "word": "_anything",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_line"
            }
          },
          "named": true,
          "value": "preamble"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "[#;].*"
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pair"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_line"
          }
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_glob_expression"
          },
          "named": true,
          "value": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_glob_expression": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "*"
            },
            "named": true,
            "value": "wildcard_chars"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "**"
            },
            "named": true,
            "value": "wildcard_chars_allow_slash"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "?"
            },
            "named": true,
            "value": "wildcard_char_single"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "/"
            },
            "named": true,
            "value": "path_separator"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\*\\?\\[\\]\\{\\}\\n]"
            },
            "named": true,
            "value": "character"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "\\\\."
            },
            "named": true,
            "value": "escaped_character"
          },
          {
            "type": "SYMBOL",
            "name": "sequence_expression"
          },
          {
            "type": "SYMBOL",
            "name": "brace_expansion"
          }
        ]
      }
    },
    "brace_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "expansion_string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "expansion_string": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PREC_RIGHT",
          "value": 0,
          "content": {
            "type": "SYMBOL",
            "name": "_glob_expression"
          }
        }
      }
    },
    "sequence_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "start",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "-?\\d+"
            },
            "named": true,
            "value": "number"
          }
        },
        {
          "type": "STRING",
          "value": ".."
        },
        {
          "type": "FIELD",
          "name": "end",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "-?\\d+"
            },
            "named": true,
            "value": "number"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^;#=\\s\\[][^=\\n]*"
            },
            "named": true,
            "value": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]*"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_anything"
          },
          "named": true,
          "value": "other"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][nN][sS][eE][tT]"
          },
          "named": true,
          "value": "unset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\d+"
          },
          "named": true,
          "value": "number"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[a-z]{2}-[A-Z]{2}"
          },
          "named": true,
          "value": "spelling_language"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[tT][rR][uU][eE]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[fF][aA][lL][sS][eE]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[oO][fF][fF]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[sS][pP][aA][cC][eE]"
          },
          "named": true,
          "value": "indent_style"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[tT][aA][bB]"
          },
          "named": true,
          "value": "indent_style"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][fF]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[cC][rR]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[cC][rR][lL][fF]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][aA][tT][iI][nN]1"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-8"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-16[bB][eE]"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-16[lL][eE]"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-8-[bB][oO][mM]"
          },
          "named": true,
          "value": "charset"
        }
      ]
    },
    "_anything": {
      "type": "PATTERN",
      "value": ".*\\S"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
