{
  "name": "editorconfig",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_line"
            }
          },
          "named": true,
          "value": "preamble"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "[#;].*"
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pair"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_line"
              }
            ]
          }
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\[\\]\\n]+"
          },
          "named": true,
          "value": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^;#=\\s\\[]+"
          },
          "named": true,
          "value": "key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "value"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "unset"
          },
          "named": true,
          "value": "unset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\d+"
          },
          "named": true,
          "value": "number"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[a-z]{2}-[A-Z]{2}"
          },
          "named": true,
          "value": "spelling_language"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[tT][rR][uU][eE]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[fF][aA][lL][sS][eE]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[oO][fF][fF]"
          },
          "named": true,
          "value": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[sS][pP][aA][cC][eE]"
          },
          "named": true,
          "value": "indent_style"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[tT][aA][bB]"
          },
          "named": true,
          "value": "indent_style"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][fF]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[cC][rR]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[cC][rR][lL][fF]"
          },
          "named": true,
          "value": "end_of_line"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[lL][aA][tT][iI][nN]1"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-8"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-16[bB][eE]"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-16[lL][eE]"
          },
          "named": true,
          "value": "charset"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[uU][tT][fF]-8-[bB][oO][mM]"
          },
          "named": true,
          "value": "charset"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
