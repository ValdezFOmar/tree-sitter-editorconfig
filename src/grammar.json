{
  "name": "editorconfig",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "any"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "_white_space"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "[#;].*"
    },
    "any": {
      "type": "PATTERN",
      "value": ".+"
    },
    "_white_space": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
